{
  "name": "@mohantalachutla/mfe_starter",
  "version": "1.0.0",
  "license": "MIT",
  "author": {
    "name": "Mohan Talachutla"
  },
  "description": "A react mfe starter app",
  "homepage": "https://github.com/mohantalachutla/mfe-starter",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/mohantalachutla/mfe-starter.git"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "main": "lib/index.js",
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "require": "./lib/index.js"
    }
  },
  "files": [
    "lib/"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "app": {
    "host": "http://localhost",
    "port": 8081
  },
  "scripts": {
    "start": "serve -s lib -p 8081",
    "start:dev": "webpack serve --mode development --live-reload --hot --config ./webpack/webpack.config.cjs ",
    "build": "webpack --mode production --config ./webpack/webpack.config.cjs ",
    "build:dev": "webpack --mode development --watch --config ./webpack/webpack.config.cjs ",
    "sb": "storybook dev -p 6006",
    "sb:build": "storybook build",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "lint:format": "prettier ./**/*.{js,jsx,ts,tsx,mjs,cjs,json} --write .",
    "lint:staged": "lint-staged",
    "lint:all": "npm run lint:fix && npm run lint:format"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "@mohantalachutla/inject-store": "^1.0.0",
    "@mohantalachutla/mfe-utils": "^1.0.0",
    "@reduxjs/toolkit": "^2.3.0",
    "axios": "^1.7.7",
    "flowbite-react": "^0.10.2",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "redux-saga": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/eslint-parser": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@chromatic-com/storybook": "^3.2.2",
    "@eslint-react/eslint-plugin": "^1.18.0",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.16.0",
    "@storybook/addon-essentials": "^8.4.3",
    "@storybook/addon-interactions": "^8.4.3",
    "@storybook/addon-onboarding": "^8.4.3",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-webpack5-compiler-swc": "^1.0.5",
    "@storybook/blocks": "^8.4.3",
    "@storybook/react": "^8.4.3",
    "@storybook/react-webpack5": "^8.4.3",
    "@storybook/test": "^8.4.3",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.9",
    "@tailwindcss/typography": "^0.5.15",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.3.0",
    "dotenv-webpack": "^8.0.1",
    "globals": "^15.13.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.48",
    "postcss-loader": "^4.1.0",
    "prettier": "^3.4.2",
    "prop-types": "^15.8.1",
    "serve": "^14.2.4",
    "storybook": "^8.4.3",
    "style-loader": "^3.3.0",
    "tailwindcss": "^3.4.14",
    "webpack": "^5.95.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^6.0.1"
  }
}
